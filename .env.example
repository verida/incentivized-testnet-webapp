# Mandatory, must be set to true to use the specific eslint config
EXTEND_ESLINT=true

# Optional: node environment mode: "production", "development" or  "test".
#NODE_ENV=development

# Optional: Set the port to run the server on. Defaults to 3000
#PORT=3000

# Optional: set to true to enable debugging features (logging, deleting data, etc.), default to false
# REACT_APP_DEV_MODE=false

# Optional: Environment of the Verida Network: "local", "testnet" (default) or "mainnet".
#REACT_APP_VERIDA_ENV=testnet

#Optional: RPC URL for the Poilygon blockchain used by the Verida Network. Optional, as the Verida protocol will fallback to a default public one, but strongly recommended as the public ones are not always reliable.
#REACT_APP_VERIDA_RPC_URL=

# Required. The URL of the API to claim the rewards
REACT_APP_XP_POINT_CLAIM_API_BASE_URL=

# Optional. The minimum amount of XP points required to claim the rewards. Default to zero
REACT_APP_MIN_XP_POINT_TO_CLAIM=200

# Optional. 'true' of 'false' to enable Sentry. Default to 'true' if undefined.
REACT_APP_SENTRY_ENABLED=

# Optional. Data Source Name required by sentry to report errors and performance issues. If undefined, Sentry will be disabled
REACT_APP_SENTRY_DSN=

# Optional. Environment used in Sentry, highly recommended.
REACT_APP_SENTRY_ENVIRONMENT=

# Optional. Number between 0 (0%) and 1.0 (100%), capture x% of the transactions for performance monitoring. Default to 0.1 (10%). Keep it low in Production, but can be set to 1.0 (100%) in development.
REACT_APP_SENTRY_TRACE_SAMPLE_RATE=0.1

# Optional. Number between 0 (0%) and 1.0 (100%), capture x% of the sessions for replay. Default to 0.1 (10%). Keep it low in Production, but can be set to 1.0 (100%) in development.
REACT_APP_SENTRY_REPLAY_SESSION_SAMPLE_RATE=0.1

# Optional. Number between 0 (0%) and 1.0 (100%), capture x% of the sessions in error for replay. Default to 1.0 (100%). Can keep it like this in Production, reduce it if too much noise or close to quota.
REACT_APP_SENTRY_REPLAYS_ON_ERROR_SAMPLE_RATE=1.0

# Optional. Define the log level among the following values, by order: "error", "warn", "info" (default), "debug"
REACT_APP_LOG_LEVEL=

# Optional. Set the site ids for plausible analytics. If undefined, plausible will be disabled
REACT_APP_PLAUSIBLE_DOMAINS=missions.verida.network

# Required. The URL of proof-connector-dapp url. The app will open this link to claim credentials using zkPass/reclaim protocol.
# If undefined, the app won't open external link to claim zkPass/reclaim proofs.
# It should not have a trailing slash (/).
REACT_APP_ZK_DAPP_CONNECTOR_BASE_URL=
